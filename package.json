{
  "name": "@wataki/wataki-sdk",
  "version": "0.1.0",
  "type": "module",
  "description": "TypeScript SDK for the Wataki WhatsApp API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "wataki",
    "whatsapp",
    "whatsapp-api",
    "sdk",
    "typescript"
  ],
  "repository": "git://github.com/Prat011/wataki-sdk.git",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.5.4"
  }
}
